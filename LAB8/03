#include <iostream>
using namespace std;

int main() {
    int n, m, p;
    cin >> n >> m >> p;

    char name[5][5][5][101];

    for(int i=0; i<n; i++) {
        for(int j=0; j<m; j++) {
            for(int k=0; k<p; k++) {
                cin >> name[i][j][k];
            }
        }
    }

    cout << "Elements in the matrix:\n";
    for(int i=0; i<n; i++) {
        for(int j=0; j<m; j++) {
            for(int k=0; k<p; k++) {
                cout << "name[" << i << "][" << j << "][" << k << "]: "
                     << name[i][j][k] << endl;
            }
        }
    }

    cout << "Elements in the matrix:\n";
    for(int i=0; i<n; i++) {
        for(int j=0; j<m; j++) {
            cout << "name[" << i << "][" << j << "]: ";
            for(int k=0; k<p; k++) {
                cout << name[i][j][k];
                if (k < p - 1) cout << " ";
            }
            cout << endl;
        }
    }

    return 0;
}
