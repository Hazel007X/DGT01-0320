#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n; 

    string str[10]; 
   
    for (int i = 0; i < n; i++) {
        cin >> str[i];
    }
  
    for (int i = n - 1; i >= 0; i--) {
        cout << str[i];
    }

    for (int i = 0; i < n; i++) {
        cout << str[i];
    }

    cout << endl;
    return 0;
}
